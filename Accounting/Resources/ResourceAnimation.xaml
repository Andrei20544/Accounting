    <!--SaveAP Button Animation-->

    <!--SaveALL Button Animation-->
    <Storyboard x:Key="IcoAnimSaveALLEnter">
        <ColorAnimation Storyboard.TargetProperty="Foreground.Color" 
                            Duration="0:0:0.1" 
                            To="LightSeaGreen"
                            Storyboard.TargetName="IcoAllSave"/>
    </Storyboard>

    <Storyboard x:Key="IcoAnimSaveALLLeave">
        <ColorAnimation Storyboard.TargetProperty="Foreground.Color" 
                            Duration="0:0:0.1" 
                            To="{x:Null}"
                            Storyboard.TargetName="IcoAllSave"/>
    </Storyboard>
    <!--SaveLL Button Animation-->

    <!--SaveALL Button Animation-->
    <Storyboard x:Key="IcoAnimDeleteDealEnter">
        <ColorAnimation Storyboard.TargetProperty="Foreground.Color" 
                            Duration="0:0:0.1" 
                            To="DarkRed"
